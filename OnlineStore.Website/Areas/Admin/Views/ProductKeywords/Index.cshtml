@model AjaxSettings

@Html.Partial("_Message")

<div class="list-items">
    @Html.Partial("_Loading")
    @Html.Partial("_ProductSimpleSearch", Model)
    <button id="AddToList" type="submit" class="btn btn-warning col-xs-1">افزودن</button>
    <div class="tags_clear"></div>
    <div class="tags_clear"></div>
    <div id="Items">
        <ul></ul>
    </div>
    <a class="icon-check-sign hide"></a>
</div>
<input type="hidden" name="ID" id="ID" value="@Request.QueryString["KeywordID"]" />

@section Styles{
    <link href="/css/admin/list-items.min.css" rel="stylesheet" />
    <link href="/css/admin/simple-search.min.css" rel="stylesheet" />
}

@section Scripts{
    <script src="/js/admin/product-simple-search.js"></script>
    <script src="~/js/admin/product-keywords.js"></script>
}
